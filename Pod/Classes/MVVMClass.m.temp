#import "NSObject+MVVMKVOPrivate.h"

@implementation MVVMTemplate

- (void)observeValueForKeyPath:(NSString *)keyPath ofObject:(id)object change:(NSDictionary<NSString *, id> *)change context:(void *)context {
    [self mvvm_observeValueForKeyPath:keyPath ofObject:object change:change context:context superClass:[MVVMTemplate superclass]];
}

- (void)dealloc {
    [self mvvm_unobserveAll];
}

@end
